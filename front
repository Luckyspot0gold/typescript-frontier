                        <pre class="text-green-400"><span class="text-gray-500">#!/bin/bash</span>
<span class="text-gray-500"># quiet_miner.sh</span>
<span class="text-blue-400">while</span> <span class="text-blue-400">true</span>; <span class="text-blue-400">do</span>
    BALANCE=$(solana balance -u mainnet-beta $WALLET | grep -oP <span class="text-yellow-300">'\d+\.\d+'</span>)
    <span class="text-blue-400">if</span> (( $(<span class="text-blue-400">echo</span> <span class="text-yellow-300">"$BALANCE > 50"</span> | bc -l) )); <span class="text-blue-400">then</span>
        solana delegate-stake $STAKE_ACCOUNT $VALIDATOR --fee-payer $WALLET
    <span class="text-blue-400">fi</span>
    <span class="text-blue-400">sleep</span> 600 <span class="text-gray-500"># 10min intervals</span>
<span class="text-blue-400">done</span></pre>
                    </div>
                </div>
                
                <!-- Auto-Start -->
                <div class="bg-gray-900 rounded-xl p-6 shadow-lg">
                    <h3 class="text-2xl font-bold mb-4 text-green-400">Auto-Start:</h3>
